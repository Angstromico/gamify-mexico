import DashboardLayout from "@layouts/DashboardLayout.astro"
import type { Games } from "@interfaces/index";

const apiLink = import.meta.env.QUIZ_API
---

<DashboardLayout currentLang="en" title="Dashboard Gamify Mexico" description="Dashboard from Gamify Mexico" menuTitle="MAIN" image="/dashEn.png">
